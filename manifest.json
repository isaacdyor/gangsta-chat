{
  "manifest_version": 3,
  "name": "Wordware Talk With Webpage",
  "description": "Talk with your webpage",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://api.wordware.ai/"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
